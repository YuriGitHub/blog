<%= form_for @user, url: '/registration', method: :post  do |f| %>


    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <br/>
    <br/>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <br/>
    <br/>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <br/>
    <br/>
    <%= f.label :password %>
    <%= f.text_field :password %>
    <br/>
    <br/>
    <%= f.label :country %>
    <%= f.text_field :country %>
    <br/>
    <br/>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <br/>
    <br/>

    <%= f.submit %>


<% end %>


